{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/pages/api/currency.js",
    "src/pages/docs/markdown-render.js",
    "src/pages/docs/scraping.tsx",
    "src/pages/docs/google-sheets.js",
    "src/components/JavascriptExample.js",
    "src/components/Scraping.js"
  ],
  "exclude": ["node_modules"]
}
